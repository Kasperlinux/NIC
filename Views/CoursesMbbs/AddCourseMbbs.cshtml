@model NIC.Models.ViewModel.CoursesMbbsViewModel
<a class="btn btn-primary" asp-area="" asp-controller="CoursesMbbs" asp-action="EditCoursesMbbs"><i class="fa-solid fa-arrow-left-to-line"></i>Go Back</a>
<form method="post" action="AddCourseMbbs" class="mt-3">
    <div class="mb-3">
        <label for="" class="form-label">Mbbs Course Code</label>
        <input type="number" class="form-control" asp-for="MbbsCourseCode" />
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Mbbs Course Name</label>
        <input type="text" class="form-control" asp-for="MbbsCouseName" />
    </div>
    <button type="submit" class="btn btn-primary">Add Mbbs Course</button>
    <a class="btn btn-primary" asp-controller="CoursesMbbs" asp-action="EditCoursesMbbs">Go Back</a>
</form>