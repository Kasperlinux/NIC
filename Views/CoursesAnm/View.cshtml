@model NIC.Models.ViewModel.CoursesAnmViewModel
@{
}
<h1>Edit Anm Courses</h1>
<a class="btn btn-primary" asp-area="" asp-controller="CoursesAnm" asp-action="EditCoursesAnm"><i class="fa-solid fa-arrow-left-to-line"></i>Go Back</a>
<form method="post" action="View" class="mt-3">
    <div class="mb-3">
        <label for="" class="form-label">Anm Course Code</label>
        <input readonly class="form-control" asp-for="AnmCourseCode" />
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Amn Course Name</label>
        <input type="text" class="form-control" asp-for="AmnCourse" />
    </div>
    <button type="submit" class="btn btn-primary">Update Amn Course</button>
    <button type="submit" class="btn btn-danger" asp-action="Delete" asp-controller="CoursesAnm">Delete </button>

</form>
