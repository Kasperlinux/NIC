@model NIC.Models.ViewModel.CoursesGnmViewModel
@{
}
<h1>Edit Gnm Courses</h1>
<a class="btn btn-primary" asp-area="" asp-controller="CoursesGnm" asp-action="EditCoursesGnm">Go Back</a>
<form method="post" action="View" class="mt-3">
    <div class="mb-3">
        <label for="" class="form-label">Gnm Course Code</label>
        <input readonly class="form-control" asp-for="GnmCourseCode" />
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Gmn Course Name</label>
        <input type="text" class="form-control" asp-for="GmnCouse" />
    </div>
    <button type="submit" class="btn btn-primary">Update Gmn Course</button>
    <button type="submit" class="btn btn-danger" asp-action="Delete" asp-controller="CoursesGnm">Delete State</button>
</form>